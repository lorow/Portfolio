{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "noImplicitAny": true,
    "target": "ES6",
    "moduleResolution": "node",
    "allowJs": true,
    "outDir": "build",
    "paths": {
      "app/*": ["./src/portfolio/*"],
    }
  }
}